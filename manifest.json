{
    "name": "Bookmarks",
    "version": "0.0.2",
    "manifest_version": 2,
    "description": "Google Bookmarks Extension",
    "browser_action" : {
        "default_popup": "popup.html"
    },
    "background": {
        "persistent": false,
        "scripts": [
            "js/background.js"
        ]
    },
    "permissions": [
        "alarms",
        "contextMenus",
        "tabs",
        "storage",
        "*://www.google.com/bookmarks/*"
    ]
}